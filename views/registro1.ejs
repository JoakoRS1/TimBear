<!DOCTYPE html>
<html>
    <head>
        <title>Tim-Bear</title>
        <meta charset="utf-8" />
        <link rel="shortcut icon" href="/imagenes/logo.png" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-F3w7mX95PdgyTmZZMECAngseQB83DfGTowi0iMjiWaeVhAn4FJkqJByhZMI3AhiU" crossorigin="anonymous">
        <link href="https://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.3/jquery.min.js">
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
        <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/knockout/3.4.2/knockout-min.js"></script>
        <script type="text/javascript"  src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa"
        crossorigin="anonymous"></script>
        <script type="text/javascript"  src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validate/1.16.0/jquery.validate.min.js"></script>
        <script type="text/javascript"   src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validation-unobtrusive/3.2.6/jquery.validate.unobtrusive.min.js"></script>
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.1/dist/js/bootstrap.bundle.min.js" integrity="sha384-/bQdsTh/da6pkI1MST/rWKFNjaCP5gBSY4sEBT38Q/9RBh9AH40zEOg7Hlq2THRZ" crossorigin="anonymous"></script>
        <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script> 
        <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-easing/1.4.1/jquery.easing.min.js" integrity="sha512-0QbL0ph8Tc8g5bLhfVzSqxe9GERORsKhIn1IrpxDAgUsbBGz/V7iSav2zzW325XGd1OMLdL4UiqRJj702IeqnQ==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
        <script src="http://www.jqueryscript.net/demo/jQuery-Plugin-For-Horizontal-Floating-Social-Share-Bar/js/jquery.floating-share.js" type="text/javascript"></script>
    </head>
    <body class="">
        <%- include('menu')  %>
    <div class="col">
        <center><h1>¡Crea una nueva cuenta!</h1></center>
        <br>
        <ul id="progressbar" role="progressbar">
            <li class="active">Paso 1</li>
            <li>Paso 2</li>
            <li>Paso 3</li>
            <li>Paso 4</li>
            <li>Paso 5</li>
        </ul>
<form id="msform1" class="msform container" method="post" action="/registro1">
            <div class="div-form" id="sf1">
                <h2 class="fs-title">Paso 1</h2>
                <center>
                    <input type="text" placeholder="Nombre" class="form-control" name='nombreU' required> 
                    <input type="text" placeholder="Apellido" class="form-control" name='apellidoU' required></center>
                <button type="button" name="sig1" class="next action-button" value="Siguiente">Siguiente</button>
            </div>
            <div class="div-form" id="sf2" >
                <h2 class="fs-title">Paso 2</h2>
                <center>
                    <label id="etiqueta" class="form-label">DNI:</label>
                    <input type="text" placeholder="Dni" class="form-control" name='dniU' required>
                </center>
                <center>
                        <label for="formFile" class="form-label">Adjuntar foto</label>
                        <input class="form-control" type="file" id="fotoU">
                </center>
                <button type="button" name="ant1" class="previous action-button-previous">Anterior</button>
                <button type="button" name="sig2" class="next action-button">Siguiente</button>
            </div>
            <div class="div-form" id="sf3">
                <h2 class="fs-title">Paso 3</h2>
                <center>
                    <input type="text" placeholder="Correo electrónico" class="form-control" name='correoU' required>
            </center>
            <center>
                    <input type="password" placeholder="Contraseña" class="form-control" name='contraU' id="upass1" required></center>
                <center>
                        <input type="password" placeholder="Confirmar contraseña" class="form-control" id="upass2" name='confirmarcontraU' required>
                    </center>
                    <center>
                            <input type="text" placeholder="Número telefónico" class="form-control" name='numeroU' required></center>

                <button type="button" name="ant2" class="previous action-button-previous">Anterior</button>
                <button type="button" name="sig3" class="next action-button">Siguiente</button>
                    </div>
            <div class="div-form" id="sf4">
                <h2 class="fs-title">Paso 4</h2>
                <center>
                    <label id="etiqueta" class="form-label">Dirección:</label>
                    <input type="text" class="form-control" name="direcU" rows="3" required>
            </center>
            <center>
                    <label id="etiqueta" class="form-label">Departamento:</label>
                    <select class="form-select" aria-label="Default select example" name="Departamento" id="Departamento">
                        <option selected>Seleccione un departamento</option>
                        <option value="1">One</option>
                        <option value="2">Two</option>
                        <option value="3">Three</option>
                      </select>
                </center>
                <center>
                        <label id="etiqueta" class="form-label">Provincia:</label>
                        <select class="form-select" name="Provincia" aria-label="Default select example" id="Provincia">
                            <option selected>Seleccione una provincia</option>
                            <option value="1">One</option>
                            <option value="2">Two</option>
                            <option value="3">Three</option>
                          </select>
                    </center>
                    <center>
                            <label id="etiqueta" class="form-label">Distrito:</label>
                            <select class="form-select" name="Distrito" aria-label="Default select example" id="Distrito">
                                <option selected>Seleccione un distrito</option>
                                <option value="1">One</option>
                                <option value="2">Two</option>
                                <option value="3">Three</option>
                              </select>
                        </center>

                <button type="button" name="ant3" class="previous action-button-previous">Anterior</button>
                <button type="button" name="sig4" class="next action-button">Siguiente</button>
                    </div>
            <div class="div-form" id="sf5">
                <h2 class="fs-title">Paso 5</h2>
                <center>
                        ¿Es una persona políticamente expuesta? (PEP)
                    <center>
                        <input class="form-check-input" type="radio" name="flexRadioDefault1" value="1" id="flexRadioDefault1">
                        <label class="form-check-label" for="flexRadioDefault1">
                          Si
                        </label>
                        <input class="form-check-input" type="radio" name="flexRadioDefault2" value="0" id="flexRadioDefault2" checked>
                        <label class="form-check-label" for="flexRadioDefault2">
                          No
                        </label>
                      </center>
                  <center>

                <button type="button" name="ant4" class="previous action-button-previous">Anterior</button>
                <button type="submit" name="sig5" class="action-button">Registrar</button>
                  </div>
        </form>
    </div>

    <script type="text/javascript">
        jQuery().ready(function() {
          // Binding next button on first step
          $(".sig1").click(function() {
              if (v.form()) {
                $(".frm").hide("fast");
                $("#sf2").show("slow");
              }
           });
           // Binding next button on second step
           $(".sig2").click(function() {
              if (v.form()) {
                $(".frm").hide("fast");
                $("#sf3").show("slow");
              }
            });
             // Binding back button on second step
            $(".ant1").click(function() {
              $(".frm").hide("fast");
              $("#sf1").show("slow");
            });
             // Binding back button on third step
             $(".ant2").click(function() {
              $(".frm").hide("fast");
              $("#sf2").show("slow");
            });
            $(".sig3").click(function() {
              if (v.form()) {
                $(".frm").hide("fast");
                $("#sf4").show("slow");
              }
            });
            $(".ant3").click(function() {
              $(".frm").hide("fast");
              $("#sf3").show("slow");
            });
            $(".ant4").click(function() {
              $(".frm").hide("fast");
              $("#sf4").show("slow");
            });
            $(".sig4").click(function() {
              if (v.form()) {
                $(".frm").hide("fast");
                $("#sf5").show("slow");
              }
            });
        });
        </script>
        
        <script type="text/javascript">
            jQuery().ready(function() {
              var v = jQuery("#basicform").validate({
                  rules: {
                    
                    upass1: {
                      required: true,
                      minlength: 6,
                      maxlength: 15,
                    },
                    upass2: {
                      required: true,
                      minlength: 6,
                      equalTo: "#upass1",
                    }
                  },
                })});
            ;
            </script>    
    
    <%- include('footer')%>

    </body>
</html>